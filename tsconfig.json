{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@modules/*": ["src/modules/*"],
      "@databases/*": ["src/databases/*"],
      "@utils/*": ["src/utils/*"]
    }
  }
}
