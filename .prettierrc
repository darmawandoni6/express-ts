{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "semi": true,
  "trailingComma": "es5",
  "printWidth": 120,
  "importOrder": [
    "^(express/(.*)$)|^(express$)",
    "<THIRD_PARTY_MODULES>",
    "^@routes(.*)$",
    "^@models(.*)$",
    "^@utils(.*)$",
    "^@(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
